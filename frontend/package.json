{
  "name": "sgics-frontend",
  "version": "1.0.0",
  "description": "Frontend for SGICS - Sistema de Gesti√≥n Integral de Cursos Scout",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:coverage:ui": "vitest --coverage --ui",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .eslintignore",
    "lint:check": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .eslintignore",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "type-check": "vue-tsc --noEmit",
    "test:e2e": "echo 'E2E tests not configured yet'",
    "perf:audit": "lhci autorun",
    "perf:collect": "lhci collect",
    "perf:assert": "lhci assert",
    "ci": "npm run lint:check && npm run format:check && npm run type-check && npm run test:run && npm run perf:audit"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.16",
    "@heroicons/vue": "^2.0.18",
    "@primevue/themes": "^4.4.1",
    "@types/leaflet": "^1.9.21",
    "axios": "^1.6.0",
    "chart.js": "^4.5.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "html5-qrcode": "^2.3.8",
    "jwt-decode": "^4.0.0",
    "leaflet": "^1.9.4",
    "pinia": "^2.1.7",
    "primeicons": "^7.0.0",
    "primevue": "^4.4.1",
    "qrcode": "^1.5.4",
    "qrcode.vue": "^3.6.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "vee-validate": "^4.11.8",
    "vue": "^3.4.0",
    "vue-chartjs": "^5.3.2",
    "vue-router": "^4.2.5",
    "vuetify": "^3.9.0-beta.1",
    "vuex": "^4.1.0",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@lhci/cli": "^0.13.0",
    "@types/node": "^24.9.2",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^4.0.6",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.0",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^23.0.0",
    "postcss": "^8.5.6",
    "prettier": "^3.0.0",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.2.0",
    "vite": "^7.1.12",
    "vite-plugin-vue-inspector": "^5.3.2",
    "vitest": "^4.0.5",
    "vue-tsc": "^3.1.1"
  }
}
